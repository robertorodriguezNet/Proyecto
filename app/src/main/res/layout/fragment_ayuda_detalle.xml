<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ayuda.fragments.almacen.AyudaAlmacenAgregarFragment">

    <TextView
        android:id="@+id/ayuda_detalle_titulo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Agregar un producto al almacÃ©n"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/ayuda_detalle_contenido"
        app:layout_constraintVertical_bias="1.0" />

    <WebView
        android:id="@+id/ayuda_detalle_contenido"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ayuda_detalle_titulo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ayuda_detalle_titulo" />


</androidx.constraintlayout.widget.ConstraintLayout>